<UserControl x:Class="Cosmos.Kernel.LogViewer.Overview.Multiboot_MMapDisplay"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ListView ItemsSource="{Binding Source={StaticResource LogFile}, XPath=//BootInfo_MMap}">
		<ListView.View>
			<GridView>
				<GridViewColumn Header="StartAddress" DisplayMemberBinding="{Binding XPath=@AddrHigh}"/>
				<GridViewColumn Header="Length" DisplayMemberBinding="{Binding XPath=@LengthHigh}"/>
				<GridViewColumn Header="Type" DisplayMemberBinding="{Binding XPath=@Type}"/>
			</GridView>
		</ListView.View>
	</ListView>
</UserControl>