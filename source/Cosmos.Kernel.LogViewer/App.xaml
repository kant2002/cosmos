<Application x:Class="Cosmos.Kernel.LogViewer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:MessageTemplates="clr-namespace:Cosmos.Kernel.LogViewer.MessageTemplates;assembly="
    StartupUri="Window1.xaml" Startup="Application_Startup">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="MessageTemplates\NormalMessageTemplate.xaml"/>
			</ResourceDictionary.MergedDictionaries>
			<XmlDataProvider x:Key="LogFile" Source="TestLog.xml"/>
			<MessageTemplates:MessageTemplateSelector x:Key="OverviewTemplateSelector"/>
		</ResourceDictionary>
	</Application.Resources>
</Application>
